{
    "manifest_version": 2,
  
    "name": "Quote Displayer",
    "description": "Provides a quote when extension icon is clicked.",
    "version": "1.0",
  
    "browser_action": {
     "default_popup": "popup/popup.html"
    },
  
      "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'",

      "icons": { "16": "/white-favicon/favicon-16x16.png",
                 "32": "/white-favicon/favicon-32x32.png"}
  
  }
  